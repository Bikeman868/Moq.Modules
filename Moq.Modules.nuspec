<?xml version="1.0"?>
<package >
  <metadata>
    <id>Moq.Modules</id>
    <version>1.0.4</version>
    <title>Moq.Modules - reusable modular mocks</title>
    <authors>Martin Halliday</authors>
    <owners>Martin Halliday</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Allows package authors to supply re-uable mocks for their library</description>
    <copyright>Copyright Martin Halliday 2014-2019</copyright>
    <releaseNotes>Included specific builds for NetStandard and NetCore frameworks</releaseNotes>
    <projectUrl>https://github.com/Bikeman868/Moq.Modules</projectUrl>
    <license type="expression">Apache-2.0</license>
    <tags>moq mock reusable</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Moq" version="4.2.1507.0118" />
      </group>
      <group targetFramework="net45">
        <dependency id="Moq" version="4.13.0" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="Moq" version="4.13.0" />
      </group>
      <group targetFramework="netcoreapp2.1">
        <dependency id="Moq" version="4.13.0" />
      </group>
    </dependencies>
    <references>
		  <reference file="Moq.Modules.dll"/>
    </references>
	  <frameworkAssemblies>
		  <frameworkAssembly assemblyName="System" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="Net40\bin\Release\Moq.Modules.dll" target="lib\net40"/>
    <file src="Net45\bin\Release\Moq.Modules.dll" target="lib\net45"/>
    <file src="NetStandard\bin\Release\netstandard2.0\Moq.Modules.dll" target="lib\netstandard2.0"/>
    <file src="NetCoreApp\bin\Release\netcoreapp2.1\Moq.Modules.dll" target="lib\netcoreapp2.1"/>
    <file src="*.md" target=""/>
    <file src="*.txt" target=""/>
  </files>
</package>
